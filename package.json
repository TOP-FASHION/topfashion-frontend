    "lint-css:fix": "stylelint src/**/*.scss --fix && prettier --write src/**/*.scss",
    "lint:js": "eslint src src/**/*.js",
    "lint-js:fix": "eslint src src/**/*.js --fix",
    "clean": "rimraf buildClient buildServer",
    "cm": "git-cz"
